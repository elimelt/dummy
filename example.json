{"openapi": "3.0.0", "info": {"title": "Pet Store API", "version": "1.0.0"}, "paths": {"/pets": {"get": {"summary": "List all pets", "responses": {"200": {"description": "A list of pets", "content": {"application/json": {"example": [{"id": 1, "name": "Dog"}, {"id": 2, "name": "Cat"}]}}}}}, "post": {"summary": "Create a new pet", "requestBody": {"required": true, "content": {"application/json": {"example": {"name": "Fish"}}}}, "responses": {"201": {"description": "Pet created successfully"}}}}, "/pets/{petId}": {"get": {"summary": "Get a pet by ID", "parameters": [{"name": "petId", "in": "path", "required": true, "schema": {"type": "integer"}}], "responses": {"200": {"description": "The requested pet", "content": {"application/json": {"example": {"id": 1, "name": "Dog"}}}}, "404": {"description": "Pet not found"}}}, "put": {"summary": "Update a pet by ID", "parameters": [{"name": "petId", "in": "path", "required": true, "schema": {"type": "integer"}}], "requestBody": {"required": true, "content": {"application/json": {"example": {"name": "Updated Dog"}}}}, "responses": {"200": {"description": "Pet updated successfully"}, "404": {"description": "Pet not found"}}}, "delete": {"summary": "Delete a pet by ID", "parameters": [{"name": "petId", "in": "path", "required": true, "schema": {"type": "integer"}}], "responses": {"204": {"description": "Pet deleted successfully"}}}}}}